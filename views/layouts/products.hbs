{{>header}}
<main>
        <div class="jumbotron">
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Producto</th>
                    <th scope="col">Precio</th>
                    <th scope="col">Imagen</th>
                  </tr>
                </thead>
                <tbody>
                   
                    {{#each products}}
                   
                  <tr>
                    <th scope="row">{{this.id}}</th>
                    <td>{{this.title}}</td>
                    <td>{{this.price}}</td>
                    <td><img src="{{this.thumbnail}}"></td>
                  </tr>
                  
                  {{/each}}
                </tbody>
              </table>
        </div>
    </main>